#include<stdio.h>
#include<conio.h>
int main()
{
    int mark;
    printf("Enter mark\n");
    scanf("%d",&mark);
    if(mark>=85 && mark<=100)
    {
        printf("Grade A");
    }
    if(mark>=70 && mark<85)
    {
        printf("Grade B");
    }
    if(mark>=55 && mark<70)
    {
        printf("Grade C");
    }
    if(mark>=40 && mark<55)
    {
        printf("Grade D");
    }
    if(mark>=0 && mark<40)
    {
        printf("Grade F");
    }
    if(mark<0 || mark>100)
    {
        printf("Enter correct mark.");
    }
    return 0;
}
